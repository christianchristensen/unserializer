<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app 
   xmlns="http://java.sun.com/xml/ns/javaee" 
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5"> 
  <display-name>Quercus PHP GAE</display-name>
  
  <servlet>
    <!-- <servlet-name>hello</servlet-name>
    <servlet-class>org.example.HelloAppEngineServlet</servlet-class> -->
		<servlet-name>quercus</servlet-name>
		<servlet-class>com.caucho.quercus.servlet.GoogleQuercusServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <!-- <servlet-name>hello</servlet-name>
    <url-pattern>/helloappengine</url-pattern> -->
		<servlet-name>quercus</servlet-name>
		<url-pattern>*.php</url-pattern>
  </servlet-mapping>

	<welcome-file-list>
	 <welcome-file>index.php</welcome-file>
	</welcome-file-list>

</web-app>
